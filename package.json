{
  "name": "handrail",
  "browser": "handrail",
  "version": "1.0.1",
  "description": "a toolset for logical disjunctions / safety for your functional pipelines",
  "main": "index.js",
  "author": "brekk",
  "license": "ISC",
  "ava": {
    "require": [
      "babel-register"
    ],
    "files": [
      "src/*.spec.js"
    ],
    "source": [
      "src/*.js"
    ],
    "concurrency": 10,
    "babel": "inherit"
  },
  "nyc": {
    "lines": 100,
    "functions": 100,
    "branches": 100,
    "statements": 100,
    "reporter": [
      "lcov",
      "text",
      "text-summary",
      "html"
    ],
    "exclude": [
      "node_modules",
      "config",
      "coverage",
      "src/index.js",
      "src/util.js",
      "src/*.spec.js",
      "src/*.fixture.js"
    ],
    "include": [
      "src"
    ],
    "all": true,
    "check-coverage": true
  },
  "devDependencies": {
    "ava": "^0.19.1",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "browserify": "^14.4.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-eslint-comments": "^1.0.0",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^4.2.2",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "get-es-imports-exports": "^1.0.7",
    "husky": "^0.13.3",
    "ljs2": "^1.0.1",
    "nps": "^5.1.0",
    "nps-utils": "^1.2.0",
    "nyc": "^10.3.2",
    "rollup-plugin-buble": "^0.15.0",
    "rollup-plugin-commonjs": "^8.0.2",
    "rollup-plugin-json": "^2.1.1",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-post-replace": "^1.0.0",
    "rollup-plugin-progress": "^0.2.1",
    "uglify-js": "^3.0.15"
  },
  "dependencies": {
    "fantasy-eithers": "^0.0.2",
    "ramda": "^0.24.0"
  },
  "scripts": {
    "help!": "nps",
    "build": "nps build",
    "dist": "nps dist",
    "transpile": "nps transpile",
    "transpile.src": "nps transpile.src",
    "lint": "nps lint",
    "meta": "nps meta",
    "meta.auto": "nps meta.auto",
    "mkdir.coverage": "nps mkdir.coverage",
    "publish": "nps publish",
    "precommit": "nps precommit",
    "test": "nps test",
    "test.es6": "nps test.es6",
    "test.log": "nps test.log",
    "cost": "nps cost"
  },
  "repository": "https://github.com/brekk/handrail.git"
}
