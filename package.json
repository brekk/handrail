{
  "name": "handrail",
  "browser": "handrail",
  "reveal": true,
  "version": "1.3.6",
  "description": "a toolset for logical disjunctions / safety for your functional pipelines",
  "main": "handrail.js",
  "author": "brekk",
  "license": "ISC",
  "jest": {
    "modulePaths": [
      "src"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "mapCoverage": true,
    "moduleNameMapper": {
      "@handrail/(.*)": "<rootDir>/src/$1.js",
      "@assertions/(.*)": "<rootDir>/src/assertions/$1.js",
      "@either/(.*)": "<rootDir>/src/either/$1.js",
      "@errors/(.*)": "<rootDir>/src/errors/$1.js"
    },
    "moduleFileExtensions": [
      "js",
      "json"
    ],
    "testMatch": [
      "**/*.spec.(jsx|js)"
    ]
  },
  "nyc": {
    "lines": 100,
    "functions": 100,
    "branches": 100,
    "statements": 100,
    "reporter": [
      "lcov",
      "text",
      "text-summary",
      "html"
    ],
    "exclude": [
      "node_modules",
      "config",
      "coverage",
      "src/index.js",
      "src/util.js",
      "src/*.spec.js",
      "src/*.fixture.js"
    ],
    "include": [
      "src"
    ],
    "all": true,
    "check-coverage": true
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.1",
    "babel-loader": "7.1.2",
    "babel-plugin-syntax-async-functions": "^6.13.0",
    "babel-plugin-syntax-jsx": "^6.18.0",
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-plugin-transform-es2015-destructuring": "^6.23.0",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "browserify": "^14.4.0",
    "cost-of-modules": "^1.0.1",
    "documentation": "^4.0.0-rc.1",
    "eslint": "4.7.2",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-babel": "4.1.2",
    "eslint-plugin-eslint-comments": "1.0.3",
    "eslint-plugin-fp": "^2.3.0",
    "eslint-plugin-import": "2.7.0",
    "eslint-plugin-node": "5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "germs": "^0.1.9",
    "get-es-imports-exports": "^1.0.7",
    "husky": "0.14.3",
    "jest": "20.0.4",
    "ljs2": "1.0.3",
    "madge": "2.2.0",
    "nps": "5.7.1",
    "nps-utils": "1.3.0",
    "nyc": "11.2.1",
    "ramda": "^0.24.1",
    "rollup-plugin-alias": "^1.3.1",
    "rollup-plugin-babel-minify": "^3.1.2",
    "rollup-plugin-babili": "3.1.1",
    "rollup-plugin-buble": "0.16.0",
    "rollup-plugin-cleanup": "^1.0.1",
    "rollup-plugin-commonjs": "8.2.1",
    "rollup-plugin-json": "2.3.0",
    "rollup-plugin-node-resolve": "^3.0.0",
    "rollup-plugin-post-replace": "^1.0.0",
    "rollup-plugin-progress": "0.4.0",
    "rollup-plugin-uglify": "^2.0.1",
    "xtrace": "0.1.2"
  },
  "dependencies": {
    "entrust": "^0.0.18",
    "f-utility": "^3.5.5",
    "fantasy-eithers": "^0.0.2"
  },
  "scripts": {
    "help!": "nps",
    "build": "nps build",
    "dist": "nps dist",
    "transpile": "nps transpile",
    "transpile.src": "nps transpile.src",
    "lint": "nps lint",
    "meta": "nps meta",
    "meta.auto": "nps meta.auto",
    "mkdir.coverage": "nps mkdir.coverage",
    "precommit": "nps care",
    "test": "nps test",
    "test.es6": "nps test.es6",
    "test.log": "nps test.log",
    "cost": "nps cost",
    "dependencies": "nps dependencies",
    "documentation": "nps documentation",
    "readme": "nps readme"
  },
  "repository": "https://github.com/brekk/handrail.git"
}
